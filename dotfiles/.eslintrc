{
  "rules": {
    "indent": [
      2,
      2
    ],
    "quotes": [
      2,
      "double"
    ],
    "linebreak-style": [
      2,
      "unix"
    ],
    "semi": [
      2,
      "always"
    ],
    "no-console": [
      2
    ],
    "space-after-keywords": [
      2
    ]
  },
  "env": {
    "browser": true
  },
  "extends": "eslint:recommended",
  "plugins": [
    "requirejs"
  ]
}
